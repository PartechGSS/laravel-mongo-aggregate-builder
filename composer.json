{
  "name": "partechgss/laravel-mongo-aggregate-builder",
  "description": "Laravel Query Builder for PHP MongoDB aggregate queries.",
  "type": "library",
  "license": "BSD-3-Clause-Clear",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "PartechGSS\\": "src/"
    }
  },
  "require": {
    "illuminate/database": "^8.0",
    "mongodb/mongodb": "^1.6",
    "php": "^7.4"
  },
  "autoload-dev": {
   "classmap": [
     "tests/"
   ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^6.0"
  },
  "scripts": {
    "test": "phpunit tests"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PartechGSS\\MongoDB\\ConnectionServiceProvider"
      ]
    }
  }
}
