{
  "name": "partechgss/laravel-mongo-aggregate-builder",
  "description": "Laravel Query Builder for PHP MongoDB aggregate queries.",
  "type": "library",
  "license": "BSD-3-Clause-Clear",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "PartechGSS\\": "src/"
    }
  },
  "require": {
    "illuminate/database": ">=6.0",
    "mongodb/mongodb": ">=1.6",
    "php": "~7.3|^8.0"
  },
  "autoload-dev": {
   "classmap": [
     "tests/"
   ]
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~7.0"
  },
  "scripts": {
    "test": "phpunit tests"
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "*"
      ]
    }
  }
}
